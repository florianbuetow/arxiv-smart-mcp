{
  "pythonVersion": "3.12",
  "pythonPlatform": "Darwin",
  "venvPath": ".",
  "venv": ".venv",
  "stubPath": "stubs",
  "include": [
    "src"
  ],
  "exclude": [
    "**/__pycache__",
    "**/.pytest_cache",
    "**/node_modules",
    "**/.venv",
    "tests",
    "tests_e2e"
  ],
  "typeCheckingMode": "strict",
  "useLibraryCodeForTypes": true,
  "reportMissingImports": true,
  "reportMissingTypeStubs": true
}
